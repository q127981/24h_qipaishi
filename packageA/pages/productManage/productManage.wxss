page {
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}

.container {
  width: 100%;
  height: 78vh;
}

.user-info {
  width: 100%;
}


.dialog-item {
  display: flex;

}

.noStoreInfo {
  font-size: 30rpx;
  text-align: center;
  margin: auto;
  color: #BCBCBC;
}

.noStoreInfo .noStore-image {
  height: 220rpx;
  width: 220rpx;
}

.store-card {
  display: flex;
  width: 96%;
  margin: 0 2%;
}

.store-card__image {
  border-radius: 0;
  width: 160rpx;
  height: 160rpx;
}

.store-card .item {
  margin-top: 30rpx;
  width: 100%;
  display: flex;
  background-color: #fff;
  padding: 10rpx;
  border-radius: 10rpx;
}



.flex-space-between {
  display: flex;
  justify-content: space-between;
}

.button-container {
  /* position: absolute; */
  /* bottom: 120rpx; */
  /* left: 50%; */
  /* transform: translateX(-50%);  */
  /* width: 100%;  */
  margin-top: 20rpx;
  margin-bottom: 100rpx;
}

.top-info {
  height: 40%;
  width: 100%;
  margin-left: 30rpx;
  display: flex;
  align-items: center;
}

.remove {
  font-size: 22rpx
}

.centerInfo {
  height: 20%;
  margin-left: 0;
}

.centerInfo text {
  margin-left: 30rpx;
  background-color: rgb(229, 250, 254);
  color: rgb(0, 153, 255);
}

.vip-blacklist-item {
  height: 40%;
  margin-left: 30rpx;
  display: flex;
  align-items: center;
}

.dialog-item {
  display: flex;
  justify-content: center;
}

.productPrice {
  color: rgba(255, 93, 93, 0.85);
}

.proButton {
  font-size: 30rpx;
  margin: 0;
  margin: 0 20rpx;
}

.button-end {
  width: 100%;
  display: flex;
  justify-content: flex-end;
}

.down {
  margin: 0;
  color: #ffba00;
  background-color: #ffe4996c;
}

.up {
  margin: 0;
  background-color: #e8f4ff;
  color: #1890ff;
}

.bottom {
  width: 430rpx;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  position: fixed;
  bottom: 30rpx;
  left: 160rpx;
  border-radius: 50rpx;
  font-size: 36rpx;
}

.fix-iphonex-button {
  bottom: 68rpx !important;
}

.fix-iphonex-button::after {
  content: ' ';
  position: fixed;
  bottom: 0 !important;
  height: 68rpx !important;
  width: 100%;
  left: 0;
  background: #f4f4f4;
}

.fix-iphonex-button.transparent::after {
  content: ' ';
  position: fixed;
  bottom: 0 !important;
  height: 68rpx !important;
  width: 100%;
  left: 0;
  background: none;
}