.info-card {
  background: #fff;
  padding: 20rpx 34rpx;
  border-radius: 17rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.info-card .top {
  display: flex;
  justify-content: space-between;
}

.info-card .top .left {
