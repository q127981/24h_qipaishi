  justify-content: space-between;
  flex-direction: column;
}

.info-card .top .left .info .name {
  font-size: 32rpx;
  font-weight: 600;
  color: #000000;
  font-style: normal;
  width: 500rpx;
  white-space: nowrap;
  /* 保证文本不会换行 */
  overflow: hidden;
  /* 隐藏溢出的内容 */
  text-overflow: ellipsis;
  /* 添加省略号表示文本被截断 */
}

.info-card .distant {
  display: flex;
  justify-content: space-around;
  align-items: center;
  width: fit-content;
  height: 40rpx;
  background: #FFFFFF;
