.reserve-box {
  width: 539rpx;
  background: linear-gradient(180deg, #FFECC7 0%, #FFFFFF 16%, #FFFFFF 100%);
  border-radius: 17rpx;
  margin: 0 auto;
  padding: 34rpx 40rpx;
}

.reserve-box .title {
  font-weight: 500;
  font-size: 37rpx;
  color: #000000;
  text-align: center;
  margin-bottom: 58rpx;
}

.reserve-box .time-line {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 34rpx;
}

.reserve-box .time-line .dot {
  width: 12rpx;
  height: 12rpx;
  background: #FFA313;
  border-radius: 50%;
  position: relative;
  z-index: 9;
}

.reserve-box .time-line .dot::before {
  content: '';
  height: 60px;
  width: 2rpx;
  background: #FFA313;
  left: calc(50% - 2rpx);
  top: -50rpx;
  position: absolute;
  z-index: -1;
}

.reserve-box .time-line .time-tag {
  color: #fff;
  background-color: #FFA313;
  width: 414rpx;
  height: 46rpx;
  border-radius: 17rpx;
  line-height: 46rpx;
  display: flex;
  justify-content: space-between;
  font-weight: 400;
  font-size: 24rpx;
  color: #FFFFFF;
  padding: 9rpx 22rpx;
  position: relative;
}

.reserve-box .time-line .time-tag::before {
  content: '';
  width: 0;
  height: 0;
  border-bottom: 15rpx solid transparent;
  border-top: 15rpx solid transparent;
  border-right: 15rpx solid #FFA313;
  border-left: 15rpx solid transparent;
  left: -29rpx;
  top: calc(50% - 15rpx);
  position: absolute;
  z-index: 3;
}

.reserve-box button {
  width: 208rpx;
  height: 59rpx;
  line-height: 59rpx;
  background: #5AAB6E;
  border-radius: 52rpx 52rpx 52rpx 52rpx;
  font-weight: 400;
  font-size: 26rpx;
  color: #FFFFFF;
  margin-top: 22rpx;
}


