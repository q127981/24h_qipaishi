/* pages/orderList/orderList.wxss */
page {
  background: #F7F7F7;
  padding-bottom: 148rpx;
}

.navigtor {
  position: fixed;
  z-index: 1;
  background: #fff;
  width: 100%;
}

.navigtor .title {
  text-align: center;
}

.navigtor .left {
  position: absolute;
  left: 30rpx;
}

.navigtor .iconfont {
  font-size: 40rpx;
}

.tabs {
  width: 100%;
  height: 45px;
}

.tabs .van-dropdown-menu {
  border-bottom: 1rpx solid #ddd;
  height: 45px;
}

.tabs .van-dropdown-menu__title {
  font-size: 30rpx !important;
}

.tabs .van-dropdown-menu {
  box-shadow: none !important;
}

.lists {
  padding: 10rpx 26rpx;
}

.lists .item {
  background: #fff;
  border-radius: 17rpx 17rpx 17rpx 17rpx;
  /* padding: 15rpx 26rpx; */
  margin: 10rpx;
  position: relative;
  overflow: hidden;
}

.lists .item .top {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 27rpx 30rpx;
  border-bottom: 2rpx solid #E3E3E3;
}

.lists .item .left {
  display: flex;
  align-items: center;
  font-size: 31rpx;
  font-weight: 500;
}

.lists .item .left image {
  width: 35rpx;
  height: 32rpx;
  margin-right: 20rpx;
}

.lists .item .left .text {
  width: 400rpx;
  white-space: nowrap;
  /* 确保文本在一行内显示 */
  overflow: hidden;
  /* 隐藏超出容器的文本 */
  text-overflow: ellipsis;
  /* 使用省略号表示被截断的文本 */
}

.lists .item .right {
  text-decoration-line: underline;
  font-weight: 500;
  font-size: 26rpx;
  color: #817F7F;
}

.lists .item .highlight {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx;
}

.lists .item .highlight .left image {
  width: 32rpx;
  height: 32rpx;
}

.lists .item .highlight .left {
  display: flex;
  align-items: center;
  font-size: 31rpx;
  font-weight: 500;
}

.lists .item .highlight .right {
  font-weight: 300;
  font-size: 32rpx;
  color: #FFA313;
  text-decoration-line: none !important;
}

.lists .item .highlight text {
  font-weight: 600;
  font-size: 32rpx;
}

.lists .item .info-line {
  font-weight: 300;
  font-size: 26rpx;
  color: #000000;
  display: flex;
  align-items: flex-end;
  padding: 10rpx 30rpx;
}

.lists .item .info-line.display-space {
  display: flex;
  justify-content: space-between;
  /* align-items: ; */
}

.lists .item .info-line .copy {
  background: var(--main-color);
  color: #fff;
  font-weight: 300;
  font-size: 21rpx;
  line-height: 36rpx;
  text-align: center;
  width: 56rpx;
  border-radius: 5rpx 5rpx 5rpx 5rpx;
  margin-left: 36rpx;
}

.lists .item .bold {
  font-weight: 500;
  font-size: 26rpx;
}

.lists .discount-tag {
  position: relative;
}

.lists .discount-tag image {
  width: 350rpx;
  height: 84rpx;
}

.lists .discount-tag view {
  position: absolute;
  left: 29%;
  top: 38%;
  font-weight: 600;
  font-size: 26rpx;
  color: #fff;
}

.lists .discount-tag view text {
  color: #F73F4C;
}

.lists .info-line .btn {
  padding: 0rpx 33rpx;
  background: var(--main-color);
  color: #fff;
  font-weight: 500;
  font-size: 30rpx;
  text-align: center;
  height: 62rpx;
  line-height: 62rpx;
  border-radius: 52rpx 52rpx 52rpx 52rpx;
  margin-top: 8rpx;
  margin-bottom: 20rpx;
}

.lists .item .status-tag {
  width: 128rpx;
  height: 112rpx;
  position: absolute;
  top: 34%;
  right: 30rpx;
}

.lists .item:last-child {
  margin-bottom: 0;
}

.lists .item .orderNo {
  font-weight: 550;
  color: #45c1faea;
}

.lists .item .info {
  font-weight: 400;
  display: flex;
  justify-content: flex-start;
}

.lists .item .info .li {
  margin: 25rpx;
}

.lists .item .roomImg {
  margin: 15rpx;
  width: 120rpx;
  height: 120rpx;
  border-radius: 10rpx;
}

.lists .item .tag {
  position: absolute;
  right: 30rpx;
  top: 15rpx;
  font-size: 26rpx;
  font-weight: bold;
  width: 100rpx;
  height: 38rpx;
  text-align: center;
  line-height: 38rpx;
  /* color: #fff; */
  border-radius: 10rpx;
}

.lists .item .tag.blue {
  color: var(--main-color);
  border: 4rpx solid var(--main-color);
}

.lists .item .tag.yellow {
  color: var(--main-orange);
  border: 4rpx solid var(--main-orange);
}

.lists .item .tag.gray {
  color: var(--main-gray);
  border: 4rpx solid var(--main-gray);
}

.lists .item .tag.finish {
  color: #da4848;
  border: 4rpx solid #da4848;
}

.lists .item .title {
  font-weight: 600;
  font-size: 30rpx;
}

.lists .item .name {
  color: #999;
  font-size: 28rpx;
}

.timeBox {
  margin: 5rpx 0;
  /* background: #dddedf; */
  border-radius: 10rpx;
  padding: 10rpx 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #108ae0bd;
}

.timeBox .time .hour {
  font-size: 32rpx;
  text-align: center;
  margin-bottom: 10rpx;
  font-weight: 600;
}

.timeBox .time .date {
  font-size: 28rpx;
}

.timeBox .total {
  width: 150rpx;
  text-align: center;
  border-width: 0;
  border-bottom: 4rpx;
  border-style: solid;
  font-size: 30rpx;
  padding-bottom: 20rpx;
}

.lists .item .bottom {
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
}

.lists .item .bottom .info {
  font-size: 24rpx;
  line-height: 40rpx;
}

.lists .item .bottom .price {
  font-size: 24rpx;
  color: red;
}

.lists .item .btns {
  display: flex;
  justify-content: flex-end;
  margin-top: 5rpx;
  margin-bottom: 5rpx;
}

.lists .item .btns .btn {
  margin: 0 0 0 20rpx;
  padding: 0;
  width: 160rpx;
  height: 60rpx;
  line-height: 58rpx;
  font-size: 26rpx;
  border-radius: 30rpx;
  background: transparent;
}

.lists .item .btns .share {
  border-width: 1rpx;
  border-style: solid;
}

.lists .item .btns .cancel {
  background: #999;
  color: #fff;
}

.dialog {
  padding: 30rpx;
}

.dialog .item {
  display: flex;
  font-size: 28rpx;
  line-height: 40rpx;
  padding: 10rpx 0;
}

.dialog .item label {
  color: #666;
  flex-shrink: 0;
}

.dialog .item text {
  font-weight: 600;
  font-size: 26rpx;
}

.dialog .item .pays {
  display: flex;
}

.dialog .item .pay .info {
  line-height: 40rpx;
  font-size: 24rpx;
  color: #666;
}

.dialog .item .pay:first-child {
  margin-right: 20rpx;
}

.dialog .item .pay radio {
  zoom: .8;
}

.dialog .item .note {
  font-size: 24rpx;
}

/* 未登录 */
.containerlogin {
  padding: 0 60rpx;
}

.photo {
  padding: 60rpx 0;
  text-align: center;
}

.photo .img {
  width: 200rpx;
  height: 200rpx;
  border-radius: 100%;
  margin: 0 auto;
  overflow: hidden;
}

.photo .img image {
  width: 100%;
  height: 100%;
}

.photo .name {
  margin-top: 20rpx;
  font-size: 32rpx;
}

.loginBtn {
  margin-top: 60rpx;
  border-radius: 50rpx;
  font-size: 28rpx;
  height: 90rpx;
  line-height: 90rpx;
}

.renewBox {
  padding: 61rpx 64rpx;
  padding-bottom: 120rpx;
}

.renewBox .title {
  font-weight: 500;
  font-size: 37rpx;
  color: #000000;
  text-align: center;
  margin-bottom: 26rpx;
}

.renewBox .line {
  font-weight: 300;
  font-size: 28rpx;
  color: #000000;
  display: flex;
  justify-content: space-between;
  height: 70rpx;
  line-height: 70rpx;
}

.renewBox .line .bold {
  font-weight: 500;
}

.renewBox .line .time {
  font-weight: 500;
  display: flex;
  width: 200rpx;
  justify-content: space-between;
  align-items: center;
}

.renewBox .line image {
  width: 34rpx;
  height: 34rpx;
}

.renewBox .divide-line {
  border: 2rpx solid #EFEFEF;
  margin: 26rpx 0;
}

.renewBox .line .left,
.line .right {
  display: flex;
  align-items: center;
}

.renewBox .line .item {
  display: flex;
  align-items: center;
}

.renewBox .line .left image {
  margin-right: 17rpx;
}

.renewBox .line .left .item image {
  width: 55rpx;
  height: 46rpx;
}

.renewBox .line .left .item text {
  margin-left: 14rpx;
  margin-right: 50rpx;
}

.renewBox .line .right-item {
  display: flex;
  align-items: center;
}

.renewBox .line .right-item image {
  width: 48rpx;
  height: 48rpx;
  margin-right: 15rpx;
}

.renewBox .line .coupon {
  display: flex;
  justify-content: flex-end;
}

.renewBox .line .coupon .price-coupon {
  color: red;
}


.line .right-item .desc {
  font-size: 22rpx;
  line-height: 22rpx;
  color: #3E3E3E;
  margin-left: 10rpx;
  margin-right: 16rpx;
}

.line .right-item .desc view {
  display: flex;
  align-items: center;
  margin-bottom: 6rpx;
}

.line .right-item .desc image {
  width: 20rpx;
  height: 20rpx;
  margin-right: 1rpx;
  margin-left: 15rpx;
}

.line .right-item .link {
  font-weight: bold;
  font-size: 21rpx;
  color: #FFA313;
  text-decoration-line: underline;
  margin: 0 24rpx;
}

.line .btn {
  width: 295rpx;
  height: 84rpx;
  background: #BCBCBC;
  border-radius: 52rpx 52rpx 52rpx 52rpx;
  color: #fff;
  text-align: center;
  line-height: 84rpx;
}

.line .btn.active {
  background: var(--main-color);
}

.mode-slot {
  width: 408rpx;
  background: #F6F6F4;
  border-radius: 35rpx 35rpx 35rpx 35rpx;
  display: flex;
  align-items: center;
  margin: 30rpx auto;
}

.mode-slot view {
  height: 52rpx;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 50%;
  font-weight: bold;
  font-size: 26rpx;
  color: #817F7F;
}

.mode-slot view.active {
  background: var(--main-color);
  color: #fff;
  border-radius: 35rpx 35rpx 35rpx 35rpx;
  box-shadow: 0rpx 5rpx 10rpx 2rpx rgba(0, 0, 0, 0.16);
  background: #5AAB6E;
}

.selector {
  width: 33rpx;
  height: 33rpx;
  border: 3rpx solid #BCBCBC;
  border-radius: 50%;
}

.selector.active {
  border: 3rpx solid #5AAB6E;
  position: relative;
}

.selector.active::after {
  content: '';
  position: absolute;
  width: 22rpx;
  height: 22rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  background-color: var(--main-color);
  border-radius: 50%;
}

.mode {
  width: 100%;
  margin-top: 30rpx;
}

.mode-container {
  display: flex;
  flex-direction: row;
  white-space: nowrap;
  min-width: 100%;
}

.mode .item {
  width: 329rpx;
  height: 160rpx;
  border-radius: 17rpx 17rpx 17rpx 17rpx;
  margin-right: 18rpx;
  font-size: 21rpx;
  padding: 10rpx 16rpx;
  border: 2rpx solid var(--main-color);
  color: var(--main-color);
  display: flex;
  justify-content: space-between;
  flex-direction: column;
  flex: 0 0 auto;
}

.mode .item.active {
  background: #5AAB6E;
  color: #fff;
}

.mode .item .top {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.mode .item .top .left {
  display: flex;
  align-items: center;
}

.mode .item .top .left .pkgName {
  font-size: 28rpx;
  font-weight: 600;
  width: 230rpx;
  /* color: rgb(255, 0, 0); */
  white-space: nowrap;
  /* 确保文本在一行内显示 */
  overflow: hidden;
  /* 隐藏超出容器的文本 */
}

.mode .item .line {
  border-top: dashed 1rpx #D6D6D6;
  height: 0rpx;
}

.mode .item .line.active {
  border-top: dashed 1rpx #fff;
}

.mode .item .tag {
  width: 80rpx;
  height: 35rpx;
  line-height: 35rpx;
  background: #ED9823;
  border-radius: 9rpx 9rpx 9rpx 9rpx;
  color: #fff;
  text-align: center;
  margin-right: 14rpx;
}

.mode .item .price {
  font-size: 32rpx;
  color: red;
}

.mode-time {
  display: flex;
  flex-direction: column;
}

.mode-time .time-items {
  display: flex;
  flex-wrap: wrap;
}

.mode-time .time-item {
  width: 105rpx;
  height: 47rpx;
  background: #FFFFFF;
  border-radius: 9rpx 9rpx 9rpx 9rpx;
  border: 2rpx solid #C3C1C1;
  font-size: 26rpx;
  line-height: 47rpx;
  text-align: center;
  margin-right: 24rpx;
  color: #817F7F;
}

.mode-time .active {
  background: #5AAB6E;
  color: #fff !important;
}

.mode-time .time-cards {
  display: flex;
  justify-content: space-between;
  margin-top: 32rpx;
}

.mode-time .time-card {
  width: 195rpx;
  height: 62rpx;
  border-radius: 9rpx 9rpx 9rpx 9rpx;
  border: 2rpx solid #BCBCBC;
  font-size: 24rpx;
  color: #817F7F;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 8rpx 0;
}

.mode-time .desc,
.mode-time .time {
  display: flex;
  justify-content: center;
  align-items: center;
}

.mode-time .time {
  font-size: 21rpx;
}

.progress {
  width: 30%;
  background-color: #EFEFEF;
  border-radius: 8rpx;
  margin: 30rpx auto;
  height: 12rpx;
  overflow: hidden;
  position: relative;
}

.progress-marker {
  width: 20%;
  background: var(--main-color);
  height: 12rpx;
  border-radius: 8rpx;
  position: absolute;
  left: 0;
}

.notes {
  width: 100%;
  font-family: PingFang SC, PingFang SC;
  font-weight: 500;
  font-size: 21rpx;
  color: #5AAB6E;
  line-height: 0rpx;
  text-align: center;
  font-style: normal;
  text-transform: none;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 20rpx;
}

.notes image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 12rpx;
}